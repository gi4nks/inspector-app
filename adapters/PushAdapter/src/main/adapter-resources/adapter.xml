<mfp:adapter name="PushAdapter"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:mfp="http://www.ibm.com/mfp/integration"
	xmlns:http="http://www.ibm.com/mfp/integration/http">

	<displayName>PushAdapter</displayName>
	<description>PushAdapter</description>

	<JAXRSApplicationClass>com.ibm.inspector.PushAdapterApplication</JAXRSApplicationClass>

	<property displayName="Server URL" name="serverUrl" defaultValue="https://mobilefoundation-inspector-server.mybluemix.net:443"/>
	<property displayName="App ID" name="appId" defaultValue="com.ibm.inspection.mfp8"/>
	<property displayName="Confidential client username" name="ccUsername" defaultValue="pushsender"/>
	<property displayName="Confidential client password" name="ccPassword" defaultValue="pushsender"/>

</mfp:adapter>
